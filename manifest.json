{
	"manifest_version": 2,
	"name": "Hurry home & 下班回家",
	"description": "平平安安上班，高高兴兴下班",
	"version": "2.1.2",
	"browser_action": {
		"default_icon": "images/icon16.png",
		"default_title": "Hurry home & 下班回家",
		"default_popup": "index.html"
	},
	"icons": {
		"16": "images/icon16.png",
		"48": "images/icon48.png",
		"128": "images/icon128.png"
	},
	"content_scripts": [
		{
			"js": [
				"./js/jquery.js",
				"./js/func.js",
				"./js/index.js",
				"./js/moment.min.js"
			],
			"matches": [
				"http://*/*",
				"https://*/*"
			]
		}
	]
}